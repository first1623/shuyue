const reportsData = [
  {
    reportId: 'report_carbon_001',
    title: '2024å¹´ä¸­å›½ç¢³ä¸­å’Œå®æ–½è·¯å¾„ç ”ç©¶æŠ¥å‘Š',
    author: 'ç¢³ä¸­å’Œç ”ç©¶é™¢',
    icon: 'ğŸ“‘',
    themeIds: ['carbon'],
    categoryIds: ['0ee59149699aa8cd015ec6a51e9fe3b1', '0ee59149699aa8cd015ec6a61793c14b', '0ee5914969a07ac601e08d0a0f194b98', '012ace5f69a07ac601e3abad2634de30'],
    pages: 128,
    type: 'ç ”ç©¶æŠ¥å‘Š',
    description: 'å…¨é¢åˆ†æä¸­å›½ç¢³ä¸­å’Œç›®æ ‡çš„å®æ–½è·¯å¾„ä¸å…³é”®ä¸¾æª',
    publishDate: '2024-01',
    viewCount: 1250,
    downloadCount: 368,
    status: true
  },
  // ... æ›´å¤šæ•°æ®
];

async function importReports() {
  const db = wx.cloud.database();
  
  for (const report of reportsData) {
    try {
      await db.collection('reports').add({
        data: {
          ...report,
          createTime: db.serverDate()
        }
      });
      console.log(`å¯¼å…¥æˆåŠŸ: ${report.title}`);
    } catch (err) {
      console.error(`å¯¼å…¥å¤±è´¥: ${report.title}`, err);
    }
  }
}

importReports();
